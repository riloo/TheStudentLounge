<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>DBtesting</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/WEB-INF/broken.html</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>chatServlet</display-name>
    <servlet-name>chatServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>chatServlet</servlet-name>
    <url-pattern>/chatServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>myChatRoomsServlet</display-name>
    <servlet-name>myChatRoomsServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myChatRoomsServlet</servlet-name>
    <url-pattern>/myChatRoomsServlet</url-pattern>
  </servlet-mapping>
</web-app>